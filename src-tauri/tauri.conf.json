{
	"$schema": "../node_modules/@tauri-apps/cli/schema.json",
	"build": {
		"beforeBuildCommand": "pnpm run build",
		"beforeDevCommand": "pnpm run dev",
		"devPath": "http://localhost:3000",
		"distDir": "../out"
	},
	"package": {
		"productName": "squh-picu-med-calculator",
		"version": "../package.json"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"window": {
				"print": true
			}
		},
		"bundle": {
			"active": true,
			"category": "DeveloperTool",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "picu-med-calculator.app",
			"longDescription": "A Tauri app to calculate PICU medications",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null
			},
			"resources": [],
			"shortDescription": "PICU Med Calculator",
			"targets": ["deb", "nsis", "msi", "app", "dmg", "updater"],
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": null
		},
		"updater": {
			"active": false
		},
		"windows": [
			{
				"fullscreen": false,
				"height": 600,
				"resizable": true,
				"title": "SQUH PICU Med Calculator",
				"width": 800
			}
		]
	}
}
